<Window x:Class="TestSort.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestSort"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Name="MV">
    <DockPanel>
        <StackPanel Orientation="Horizontal" DockPanel.Dock="Top">
            <TextBlock>Find:</TextBlock>
            <TextBox Width="50"></TextBox>
        </StackPanel>
        <ListBox Name="LB" ItemsSource="{Binding Collect}"  SelectedIndex="{Binding FocusInd}">
        </ListBox>
    </DockPanel>

</Window>
